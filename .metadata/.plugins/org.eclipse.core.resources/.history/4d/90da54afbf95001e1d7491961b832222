package EndSemExam2;

public class string {
	string(){
		
	}
	string(int a){
		
	}
public static void main(String [] args) {
	String s="sajan shrestha";
	System.out.println(s.substring(1));
	System.out.println(s.substring(1,3));
	System.out.println(s.lastIndexOf('a',2));
}
}
class a extends string{
	a(int x){
		super(x);
	}
}